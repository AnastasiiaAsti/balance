<%- include('../partials/header') %>
    
<div class="parent">
    <% asanas.forEach(function(asana) { %>
                <div class="col-md-4">
                    <ul class="p-4">
                        <li><%= asana.name %></li>
                        <li><%= asana.originName %></li>
                        <li><%= asana.typeOfAsana %></li>
                        <li><a href="/asanas/<%= asana._id %>">DESCRIPTION</a></li>
                    </ul> </div>
                <% }); %>
</div>

