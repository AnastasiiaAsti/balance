<%- include('../partials/header') %>

    <div class="parent container col-12" id="parent">
        <% asanas.forEach(function(asana) { %>
            <div class="col-6">
                <ul class="p-4">
                    <li class="heading">
                        <%= asana.name %>
                    </li>
                    <li class="italic">
                        <%= asana.typeOfAsana %>
                    </li>
                    <li><a href="/asanas/<%= asana._id %>">DESCRIPTION</a></li>
                </ul>
            </div>
            <% }); %>
    </div>

    <%- include('../partials/footer') %>